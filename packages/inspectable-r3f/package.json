{
    "name": "inspectable-r3f",
    "version": "0.1.2",
    "description": "Inspect HTML textures on React Three Fiber meshes",
    "author": "Irfanul Majumder",
    "type": "module",
    "main": "./dist/inspectable-r3f.umd.cjs",
    "module": "./dist/inspectable-r3f.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/inspectable-r3f.js",
            "require": "./dist/inspectable-r3f.umd.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite build --watch"
    },
    "peerDependencies": {
        "@react-three/fiber": ">=8.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "three": ">=0.150.0"
    },
    "dependencies": {
        "html2canvas": "^1.4.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/three": "^0.160.0",
        "@vitejs/plugin-react": "^4.2.1",
        "typescript": "^5.3.0",
        "vite": "^6.0.0",
        "vite-plugin-dts": "^4.0.0"
    },
    "keywords": [
        "react",
        "three",
        "r3f",
        "react-three-fiber",
        "inspector",
        "devtools",
        "html2canvas",
        "texture"
    ],
    "license": "MIT",
    "homepage": "https://github.com/IrfanulM/InspectableR3F#readme",
    "bugs": {
        "url": "https://github.com/IrfanulM/InspectableR3F/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/IrfanulM/InspectableR3F"
    }
}